{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": [
    "google",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": [
    "prettier",
    "@typescript-eslint"
  ],
  "rules": {
    "require-jsdoc": "off",
    "prettier/prettier": "error"
  }
}
